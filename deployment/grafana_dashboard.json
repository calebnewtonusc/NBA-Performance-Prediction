{
  "dashboard": {
    "title": "NBA Prediction API - Performance Dashboard",
    "tags": ["nba", "ml", "api", "production"],
    "timezone": "browser",
    "editable": true,
    "graphTooltip": 1,
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "API Health Status",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "up{job=\"nba-api\"}",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "none",
          "colorMode": "background",
          "textMode": "value_and_name"
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {"type": "value", "value": "1", "text": "Healthy"},
              {"type": "value", "value": "0", "text": "Down"}
            ],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 1, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Total Predictions",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 0},
        "targets": [
          {
            "expr": "nba_predictions_total",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "short"
          }
        }
      },
      {
        "id": 3,
        "title": "Cache Hit Rate",
        "type": "gauge",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "nba_cache_hit_rate",
            "refId": "A"
          }
        ],
        "options": {
          "showThresholdLabels": false,
          "showThresholdMarkers": true
        },
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 1,
            "unit": "percentunit",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 0.2, "color": "yellow"},
                {"value": 0.4, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "Models Loaded",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 0},
        "targets": [
          {
            "expr": "nba_models_loaded",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "none",
          "colorMode": "value"
        }
      },
      {
        "id": 5,
        "title": "Request Rate (req/min)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 4},
        "targets": [
          {
            "expr": "rate(http_requests_total{job=\"nba-api\"}[1m]) * 60",
            "legendFormat": "{{method}} {{endpoint}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Requests/min"},
          {"format": "short"}
        ]
      },
      {
        "id": 6,
        "title": "Response Time (P50, P95, P99)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 4},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket{job=\"nba-api\"}[5m]))",
            "legendFormat": "P50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{job=\"nba-api\"}[5m]))",
            "legendFormat": "P95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(http_request_duration_seconds_bucket{job=\"nba-api\"}[5m]))",
            "legendFormat": "P99",
            "refId": "C"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Response Time"},
          {"format": "short"}
        ]
      },
      {
        "id": 7,
        "title": "Error Rate",
        "type": "graph",
        "gridPos": {"h": 6, "w": 12, "x": 0, "y": 12},
        "targets": [
          {
            "expr": "rate(nba_errors_total[5m])",
            "legendFormat": "Errors/sec",
            "refId": "A"
          },
          {
            "expr": "rate(http_requests_total{job=\"nba-api\",status=~\"5..\"}[5m])",
            "legendFormat": "5xx Errors/sec",
            "refId": "B"
          }
        ],
        "alert": {
          "name": "High Error Rate",
          "conditions": [
            {
              "evaluator": {"type": "gt", "params": [0.05]},
              "query": {"params": ["A", "5m", "now"]},
              "reducer": {"type": "avg"}
            }
          ],
          "frequency": "1m",
          "handler": 1,
          "message": "Error rate above 5%!",
          "noDataState": "no_data",
          "executionErrorState": "alerting"
        }
      },
      {
        "id": 8,
        "title": "Cache Performance",
        "type": "graph",
        "gridPos": {"h": 6, "w": 12, "x": 12, "y": 12},
        "targets": [
          {
            "expr": "nba_cache_hits",
            "legendFormat": "Cache Hits",
            "refId": "A"
          },
          {
            "expr": "nba_cache_misses",
            "legendFormat": "Cache Misses",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Count"},
          {"format": "short"}
        ]
      },
      {
        "id": 9,
        "title": "Prediction Types Distribution",
        "type": "piechart",
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 18},
        "targets": [
          {
            "expr": "sum by (prediction_type) (nba_predictions_total)",
            "legendFormat": "{{prediction_type}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 10,
        "title": "Model Usage",
        "type": "piechart",
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 18},
        "targets": [
          {
            "expr": "sum by (model_name) (nba_predictions_by_model)",
            "legendFormat": "{{model_name}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 11,
        "title": "Database Connection Pool",
        "type": "graph",
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 18},
        "targets": [
          {
            "expr": "nba_db_pool_size",
            "legendFormat": "Pool Size",
            "refId": "A"
          },
          {
            "expr": "nba_db_pool_checked_out",
            "legendFormat": "Checked Out",
            "refId": "B"
          },
          {
            "expr": "nba_db_pool_overflow",
            "legendFormat": "Overflow",
            "refId": "C"
          }
        ]
      },
      {
        "id": 12,
        "title": "Top Endpoints by Request Count",
        "type": "table",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 26},
        "targets": [
          {
            "expr": "topk(10, sum by (endpoint) (rate(http_requests_total{job=\"nba-api\"}[1h])))",
            "format": "table",
            "instant": true,
            "refId": "A"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {"Time": true},
              "renameByName": {
                "endpoint": "Endpoint",
                "Value": "Requests/sec"
              }
            }
          }
        ]
      },
      {
        "id": 13,
        "title": "Memory Usage",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 26},
        "targets": [
          {
            "expr": "process_resident_memory_bytes{job=\"nba-api\"}",
            "legendFormat": "Memory Usage",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "bytes", "label": "Memory"},
          {"format": "short"}
        ]
      },
      {
        "id": 14,
        "title": "CPU Usage",
        "type": "graph",
        "gridPos": {"h": 6, "w": 8, "x": 0, "y": 34},
        "targets": [
          {
            "expr": "rate(process_cpu_seconds_total{job=\"nba-api\"}[5m]) * 100",
            "legendFormat": "CPU %",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "percent", "max": 100, "min": 0},
          {"format": "short"}
        ]
      },
      {
        "id": 15,
        "title": "Active Connections",
        "type": "graph",
        "gridPos": {"h": 6, "w": 8, "x": 8, "y": 34},
        "targets": [
          {
            "expr": "nba_active_connections",
            "legendFormat": "Active",
            "refId": "A"
          }
        ]
      },
      {
        "id": 16,
        "title": "Uptime",
        "type": "stat",
        "gridPos": {"h": 6, "w": 8, "x": 16, "y": 34},
        "targets": [
          {
            "expr": "nba_uptime_seconds",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "none",
          "colorMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 3600, "color": "yellow"},
                {"value": 86400, "color": "green"}
              ]
            }
          }
        }
      }
    ],
    "annotations": {
      "list": [
        {
          "datasource": "Prometheus",
          "enable": true,
          "expr": "ALERTS{alertstate=\"firing\", job=\"nba-api\"}",
          "iconColor": "red",
          "name": "Alerts",
          "tagKeys": "alertname",
          "textFormat": "{{alertname}}",
          "titleFormat": "Alert"
        }
      ]
    },
    "templating": {
      "list": [
        {
          "name": "datasource",
          "type": "datasource",
          "query": "prometheus",
          "current": {
            "text": "Prometheus",
            "value": "Prometheus"
          }
        },
        {
          "name": "instance",
          "type": "query",
          "datasource": "${datasource}",
          "query": "label_values(up{job=\"nba-api\"}, instance)",
          "refresh": 1,
          "multi": false,
          "includeAll": false
        }
      ]
    }
  },
  "meta": {
    "type": "db",
    "canSave": true,
    "canEdit": true,
    "canAdmin": true,
    "expires": "0001-01-01T00:00:00Z",
    "created": "2026-02-06T14:00:00Z",
    "updated": "2026-02-06T14:00:00Z",
    "createdBy": "admin",
    "version": 1
  }
}
